#!/bin/bash
# SPDX-License-Identifier: GPL-2.0-or-later
# Copyright (C) 2021-present pkegg

. /etc/profile
DIR=$(realpath $(dirname $0))
BAND=$(get_ee_setting updates.type)

if UPDATE=$(python3 ${DIR}/get-release.py --org pkegg --band ${BAND} --check True 2>/dev/null); then
   echo "${UPDATE}"
else
   echo "no"
   exit 1
fi 